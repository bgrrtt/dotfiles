#!/usr/bin/env zsh

# echo "RUN: stow -d .dotfiles -t $HOME -R ..."

() {

    stow -d $HOME/.dotfiles -t $HOME -R alacritty
    echo "RUN: stow -d .dotfiles -t $HOME -R alacritty"
    stow -d $HOME/.dotfiles -t $HOME -R alfred
    echo "RUN: stow -d .dotfiles -t $HOME -R alfred"
    stow -d $HOME/.dotfiles -t $HOME -R brew
    echo "RUN: stow -d .dotfiles -t $HOME -R brew"
    stow -d $HOME/.dotfiles -t $HOME -R dotfiles
    echo "RUN: stow -d .dotfiles -t $HOME -R dotfiles"
    stow -d $HOME/.dotfiles -t $HOME -R git
    echo "RUN: stow -d .dotfiles -t $HOME -R git"
    stow -d $HOME/.dotfiles -t $HOME -R hammerspoon
    echo "RUN: stow -d .dotfiles -t $HOME -R hammerspoon"
    stow -d $HOME/.dotfiles -t $HOME -R karabiner
    echo "RUN: stow -d .dotfiles -t $HOME -R karabiner"
    stow -d $HOME/.dotfiles -t $HOME -R macos
    echo "RUN: stow -d .dotfiles -t $HOME -R macos"
    stow -d $HOME/.dotfiles -t $HOME -R nvim
    echo "RUN: stow -d .dotfiles -t $HOME -R nvim"
    stow -d $HOME/.dotfiles -t $HOME -R rclone
    echo "RUN: stow -d .dotfiles -t $HOME -R rclone"
    stow -d $HOME/.dotfiles -t $HOME -R sshrc
    echo "RUN: stow -d .dotfiles -t $HOME -R sshrc"
    stow -d $HOME/.dotfiles -t $HOME -R tmux
    echo "RUN: stow -d .dotfiles -t $HOME -R tmux"
    stow -d $HOME/.dotfiles -t $HOME -R vscode
    echo "RUN: stow -d .dotfiles -t $HOME -R vscode"
    stow -d $HOME/.dotfiles -t $HOME -R zsh
    echo "RUN: stow -d .dotfiles -t $HOME -R zsh"

}
