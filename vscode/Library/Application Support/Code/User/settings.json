{
    "breadcrumbs.enabled": true,
    "editor.autoIndent": "full",
    "editor.detectIndentation": true,
    "editor.fontFamily": "'Fira Code', Menlo, Monaco, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.formatOnSave": false,
    "[markdown]": {
        "editor.formatOnSave": true
    },
    "[php]": {
        "editor.formatOnSave": true
    },
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderLineHighlight": "all",
    "editor.wordWrap": "off",
    "emmet.triggerExpansionOnTab": true,
    "emmet.excludeLanguages": [
        "php"
    ],
    "emmet.includeLanguages": {
        "twig": "html",
        "php": "html"
    },
    "twigcs.executablePath": "/Users/bgrrtt/.composer/vendor/bin/twigcs",
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.html": "twig",
        "*.html.twig": "twig"
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/plugged/**": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": false,
        "**/vendor": false,
    },
    "files.trimTrailingWhitespace": true,
    "git.path": "/usr/local/bin/git",
    "gitlens.defaultDateStyle": "absolute",
    "todo-tree.highlights.enabled": false,
    "window.title": "${rootName}",
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Solarized Dark",
    "workbench.editor.showTabs": false,
    "workbench.settings.editor": "json",
    "workbench.settings.openDefaultSettings": true,
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "welcomePage",
    "workspaceExplorer.workspaceStorageDirectory": "/Users/bgrrtt/.dotfiles/vscode/.vscode/workspaces",
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.statusBarColorControl": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                ":wq"
            ]
        }
    ],
