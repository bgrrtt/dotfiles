{
    "[html]": {
        "editor.formatOnSave": false
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.formatOnSave": true,
        "editor.wordWrap": "off"
    },
    "[php]": {
        "editor.formatOnSave": true
    },
    "[twig]": {
        "editor.formatOnSave": false
    },
    "autofold.default": 2,
    "bracketPairColorizer.showBracketsInGutter": true,
    "bracketPairColorizer.showBracketsInRuler": false,
    "breadcrumbs.enabled": true,
    "cSpell.dictionaries": [
        "privateTerms"
    ],
    "cSpell.dictionaryDefinitions": [
        {
            "name": "privateTerms",
            "path": "/Users/bgrrtt/.dotfiles/vscode/.vscode/dictionaries/private.txt"
        }
    ],
    "cSpell.enableFiletypes": [
        "twig"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.autoIndent": "advanced",
    "editor.codeLens": false,
    "editor.comments.ignoreEmptyLines": false,
    "editor.comments.includeEmptyLines": true,
    "editor.cursorBlinking": "phase",
    "editor.detectIndentation": true,
    "editor.fontFamily": "'FiraCode Nerd Font', Menlo, Monaco, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renameOnType": true,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.suggestSelection": "first",
    "editor.wordWrap": "off",
    "emmet.excludeLanguages": [
        "php"
    ],
    "emmet.includeLanguages": {
        "php": "html",
        "twig": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.html": "twig",
        "*.html.twig": "twig",
        "*.twig": "twig",
        "*.yml": "yaml"
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true,
        "**/node_modules": false,
        "**/vendor": false
    },
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/plugged/**": true
    },
    "git.path": "/usr/local/bin/git",
    "gitlens.defaultDateStyle": "absolute",
    "gitlens.statusBar.reduceFlicker": true,
    "launch": {
        "compounds": [],
        "configurations": []
    },
    "php.suggest.basic": false,
    "php.validate.executablePath": "/usr/local/bin/php",
    "phpcs.standard": "PSR12",
    "projectManager.git.baseFolders": [
        "~/Work"
    ],
    "tabnine.experimentalAutoImports": 0,
    "todo-tree.highlights.enabled": false,
    "todo-tree.tree.showScanModeButton": false,
    "twig-language-2.bracePadding": true,
    "twig-language-2.commentLine": true,
    "twig-language-2.correct": false,
    "twig-language-2.indentStyle": "space",
    "twig-language-2.newLine": false,
    "twigcs.executablePath": "./vendor/bin/twigcs",
    "vim.easymotion": false,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.insertModeKeyBindings": [],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                ":wq"
            ]
        }
    ],
    "vim.sneak": false,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.commandlineinprogress": "#007acc",
    "vim.statusBarColors.easymotioninputmode": "#007acc",
    "vim.statusBarColors.easymotionmode": "#007acc",
    "vim.statusBarColors.insert": "#5f0000",
    "vim.statusBarColors.normal": "#005f5f",
    "vim.statusBarColors.replace": "#00000",
    "vim.statusBarColors.searchinprogressmode": "#007acc",
    "vim.statusBarColors.surroundinputmode": "#007acc",
    "vim.statusBarColors.visual": "#5f00af",
    "vim.statusBarColors.visualblock": "#86592d",
    "vim.statusBarColors.visualline": "#005f5f",
    "vim.useCtrlKeys": false,
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": false,
    "vim.vimrc.path": "~/.vscode/vimrc.vim",
    "vim.visualModeKeyBindings": [],
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.title": "${rootName}",
    "window.zoomLevel": -1,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#586e75",
        "statusBar.background": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f"
    },
    "workbench.colorTheme": "Better Solarized Dark Italics",
    "workbench.editor.showTabs": false,
    "workbench.settings.editor": "json",
    "workbench.settings.openDefaultSettings": true,
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "welcomePage",
    "workbench.view.experimental.allowMovingToNewContainer": true,
    "workspaceExplorer.workspaceStorageDirectory": "/Users/bgrrtt/.dotfiles/vscode/.vscode/workspaces",
    "zenMode.hideActivityBar": true,
    "zenMode.hideLineNumbers": true,
    "zenMode.hideStatusBar": true
}
